{
  "name": "tcg-logger",
  "version": "1.1.2",
  "description": "Bunyan Logger for TCG apps",
  "keywords": [
    "log",
    "json",
    "logger",
    "middleware"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "index.js"
  ],
  "author": "Joe Furfaro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-control-group/tcg-logger-express.git"
  },
  "engines": {
    "node": ">=7.0"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^1.0.7",
    "response-time": "^2.3.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^8.38.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "strict": 0,
      "indent": [
        2,
        "tab",
        {
          "SwitchCase": 1
        }
      ],
      "comma-dangle": 2,
      "no-unused-vars": [
        2,
        {
          "args": "none"
        }
      ],
      "no-console": 0,
      "no-var": 2,
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "semi": [
        2,
        "always"
      ],
      "space-infix-ops": 0,
      "max-len": [
        2,
        200,
        2
      ],
      "padded-blocks": 0,
      "no-multiple-empty-lines": 0
    }
  }
}
